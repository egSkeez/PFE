package xtensus.ldap.model;

import java.util.List;

public class Group implements java.io.Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 8941157934618486120L;
	private int id;
	private String cn;
	private String description;
	private List<Person> listUser;

	public Group() {

	}

	public Group(int id, String cn, String description) {
		this.id = id;
		this.cn = cn;
		this.description = description;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getCn() {
		return cn;
	}

	public void setCn(String cn) {
		this.cn = cn;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public List<Person> getListUser() {
		return listUser;
	}

	public void setListUser(List<Person> listUser) {
		this.listUser = listUser;
	}

}
