<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-
transitional.dtd">

<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich">
	<html>
<head>
<title>Gestion Urgence</title>

<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<script src="../framGraphique/#{languageManager.calendar}"
	type="text/javascript"></script>
</head>
<body>
	<!-- ********************************* -->
	<f:view locale="#{languageManager.locale}">
		<link rel="STYLESHEET" type="text/css"
			href="../framGraphique/#{languageManager.css}" title="css"></link>
		<div id="page">
			<div id="header">
				<ui:include src="../framGraphique/header.xhtml"></ui:include>
			</div>
			<div id="menuPrincipale">
				<ui:include src="../framGraphique/toolbar.xhtml"></ui:include>
			</div>
			<div id="contenu">
				<div id="corpsGauche">
					<rich:panel styleClass="rich-panel" style="height:30px; padding-top:5px;">
						<h:form>
							<div>
								<rich:spacer width="5px" />
								<a4j:commandLink id="retour"
									onclick="document.location.href='GestionGBO0.jsf'">
									<h:graphicImage
										value="../framGraphique/images/retour.png"
										onmouseover="this.src='../framGraphique/images/retourMouseOver.png'"
										onmouseout="this.src='../framGraphique/images/retour.png'" />
								</a4j:commandLink>
								<rich:toolTip for="retour" value="#{msg.Retour}"
									direction="top-right" />
								<rich:spacer width="5px" />
								<a4j:commandLink id="upd1"
									action="#{urgenceGestionBean.viderchamp}"
									reRender="accor1,accoruser1,accor2,accoruser2,buttonDelUpUser,buttonValidateUser,Label,
									btnAnnull,btnAnnul,btnAjout,btnModif,Identifiant,Nom,outputAjout">
									<h:graphicImage id="add1"
										value="../framGraphique/images/icone-add.png"
										onmouseover="this.src='../framGraphique/images/icone-addMouseOver.png'"
										onmouseout="this.src='../framGraphique/images/icone-add.png'" />
								</a4j:commandLink>
								<rich:toolTip for="upd1" direction="top-right"
									value="#{msg.labelAjoutUrgence}" />
							</div>
							<a4j:keepAlive beanName="urgenceGestionBean" />
						</h:form>
					</rich:panel>
					<div>
						<h:form id="accoruser1"
							style="display:#{urgenceGestionBean.hideAccor};">
							<a4j:keepAlive beanName="urgenceGestionBean" ajaxOnly="true" />
							<rich:simpleTogglePanel id="accor1" styleClass="rich-panel"
								switchType="client" label="#{urgenceGestionBean.titleaccor}"
								opened="#{urgenceGestionBean.openaccor1}">
								<rich:messages ajaxRendered="true" errorClass="msgError"
									styleClass="error" />
								<table width="100%">
									<tr>
										<td width="50%" valign="top"><h:panelGrid columns="2">
												<h:column>
													<h:outputText value="#{msg.urgenceGestionLibelle} :"  styleClass="xteDetailLabel" />
													<rich:spacer width="5" />
													<h:outputText value="*" styleClass="error" />
												</h:column>
												<h:column>
												<h:inputText id="Nom"
													value="#{urgenceGestionBean.urgence.urgenceLibelle}"
													required="true"
													requiredMessage="* #{msg.urgenceGestionLibelle} #{msg.champsobligatoire}"
													maxlength="30" />
													</h:column>
													
													<h:column>
													<h:outputText value="#{msg.urgenceGestionDescription} :"
														styleClass="xteDetailLabel" />
													<rich:spacer width="5" />
												</h:column>
												<h:inputTextarea id="Description"
													value="#{urgenceGestionBean.urgence.urgenceDescription}"
													styleClass="xteDetailLabelTextArea" rows="2"
													style=" width : 200px;" />
											</h:panelGrid></td>
										<td width="50%" valign="top"><h:panelGrid columns="2">
												<h:column>
													<h:outputText value="#{msg.armoireLibelleAr} :"
											styleClass="xteDetailLabel" />
										<rich:spacer width="10" />
										<h:outputText value="*" styleClass="error" />
									</h:column>
									<h:column>
									<h:inputText id="LibelleAr"
										value="#{urgenceGestionBean.urgence.urgenceLibelleAr}"
										required="true"
										requiredMessage="* #{msg.armoireLibelleAr} #{msg.champsobligatoire}" />
													</h:column>
													<h:column>
													<h:outputText value="#{msg.descriptionAr} :"
														styleClass="xteDetailLabel" />
													<rich:spacer width="5" />
												</h:column>
												<h:inputTextarea id="DescriptionAr"
													value="#{urgenceGestionBean.urgence.urgenceDescriptionAr}"
													styleClass="xteDetailLabelTextArea" rows="2"
													style=" width : 200px;" />
											</h:panelGrid></td>
									</tr>
								</table>
								<rich:spacer height="10" />
								<div align="center">
									<a4j:commandButton id="btnAjout"
										style="display:#{urgenceGestionBean.divajouter};"
										value="#{msg.buttonAjouter}" type="submit"
										action="#{urgenceGestionBean.save}"
										oncomplete="if(#{urgenceGestionBean.status}==true) #{rich:component('msgPanal')}.show();"
										reRender="msgPanal"></a4j:commandButton>

									<a4j:commandButton id="btnModif"
										style="display:#{urgenceGestionBean.divmodifier};"
										value="#{msg.buttonModifier}" type="submit"
										action="#{urgenceGestionBean.updateSelectedRow}"
										oncomplete="if(#{urgenceGestionBean.status1}==true) #{rich:component('askUpdate')}.show();
										 if(#{urgenceGestionBean.status2}==true) #{rich:component('msgPanalErreur')}.show();"
										reRender="btnAnnull,btnAnnul,btnAjout,btnModif,Identifiant,Nom,outputAjout,
										accor1,accoruser1,accor2,accoruser2,buttonDelUpUser,buttonValidateUser,Label"></a4j:commandButton>
								</div>


								<rich:spacer height="10" />
								<div align="left">
									<h:outputText value="#{msg.infoChampObligatoir}" />
								</div>
							</rich:simpleTogglePanel>
						</h:form>
								<!-- *******popup Info Validation d'ajout -->
								<rich:modalPanel id="msgPanalCA" autosized="true" height="90"
									width="350">
									<a4j:keepAlive beanName="urgenceGestionBean" ajaxOnly="true" />
									<f:facet name="header">
										<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
											<rich:spacer width="2" />
											<h:outputText value="#{msg.labelInfo}"
												styleClass="xtePopupInformationTextHeader" />
										</h:panelGrid>
									</f:facet>
									<f:facet name="controls">
										<h:panelGroup>
											<h:graphicImage value="../framGraphique/images/close.png"
												width="15" height="15" id="hidelinkCA" />
											<rich:componentControl for="msgPanalCA" attachTo="hidelinkCA"
												operation="hide" event="onclick" />
										</h:panelGroup>
									</f:facet>
									<h:form>
										<table align="center">
											<tr>
												<td align="left"><h:graphicImage
														value="../framGraphique/images/information.png" /></td>
												<td align="center" colspan="2"><h:outputText
														styleClass="xtePopupInformationText"
														value="#{msg.confirmAjout}" /></td>
											</tr>
										</table>
										<div align="center">
											<a4j:commandButton value="#{msg.buttonOk}"
												oncomplete="Richfaces.hideModalPanel('msgPanalCA');"
												type="submit" ajaxSingle="true" styleClass="submitButton"
												action="#{urgenceGestionBean.viderchamp}"
												reRender="accor1,accoruser1,accor2,accoruser2,buttonDelUpUser,buttonValidateUser,Label,data,scroller,record" />
										</div>
									</h:form>
								</rich:modalPanel>
								<!-- ***************************popup Update ************************** -->

								<rich:modalPanel id="askUpdate" autosized="true" height="90"
									width="350">
									<a4j:keepAlive beanName="urgenceGestionBean" />
									<f:facet name="header">
										<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
											<h:outputText value="#{msg.labelInfo}"
												styleClass="xtePopupInformationTextHeader" />
										</h:panelGrid>
									</f:facet>
									<f:facet name="controls">
										<h:panelGroup>
											<h:graphicImage value="../framGraphique/images/close.png"
												width="15" height="15" id="hidelink3" alt="Fermer" />
											<rich:componentControl for="askUpdate" attachTo="hidelink3"
												operation="hide" event="onclick" />
										</h:panelGroup>
									</f:facet>
									<h:form>

										<table align="center">
											<tr>
												<td align="left"><h:graphicImage
														value="../framGraphique/images/information.png" /></td>
												<td align="center" colspan="2"><h:outputText
														styleClass="xtePopupInformationText"
														value="#{msg.confirmUpdate}" />
												</td>
											</tr>
										</table>
										<div align="center">
											<a4j:commandButton value="#{msg.buttonOk}"
												action="#{urgenceGestionBean.updateSelectedRow}"
												oncomplete="Richfaces.hideModalPanel('askUpdate');"
												type="submit" ajaxSingle="true" styleClass="submitButton" />
										</div>
										<rich:spacer width="10" />

									</h:form>
								</rich:modalPanel>
								<!-- *****popup Erreur d'ajout remaining name****** -->
								<rich:modalPanel id="msgPanalErrRemainName" autosized="true"
									height="90" width="350">
									<a4j:keepAlive beanName="urgenceGestionBean" />
									<f:facet name="header">
										<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
											<rich:spacer width="2" />
											<h:outputText value="#{msg.labelError}"
												styleClass="xtePopupInformationTextHeader" />
										</h:panelGrid>
									</f:facet>
									<f:facet name="controls">
										<h:panelGroup>
											<h:graphicImage value="../framGraphique/images/close.png"
												width="15" height="15" id="hidelinkErrRemainName" />
											<rich:componentControl for="msgPanalErrRemainName"
												attachTo="hidelinkErrRemainName" operation="hide"
												event="onclick" />
										</h:panelGroup>
									</f:facet>
									<h:form>
										<table align="center">
											<tr>
												<td align="left"><h:graphicImage
														value="../framGraphique/images/erreur.png" /></td>
												<td align="center" colspan="2"><h:outputText
														styleClass="xtePopupInformationText"
														value="#{urgenceGestionBean.message}" /></td>
											</tr>
										</table>
										<div align="center">
											<a4j:commandButton value="#{msg.buttonOk}"
												onclick="Richfaces.hideModalPanel('msgPanalErrRemainName');"
												type="submit" ajaxSingle="true" styleClass="submitButton" />
										</div>
									</h:form>
								</rich:modalPanel>
								<!-- ******************popup validation de modification*********************** -->
								<rich:modalPanel id="msgPanal" autosized="true" height="90"
									width="350">
									<a4j:keepAlive beanName="urgenceGestionBean" />
									<f:facet name="header">
										<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
											<rich:spacer width="2" />
											<h:outputText value="#{msg.labelInfo}"
												styleClass="xtePopupInformationTextHeader" />
										</h:panelGrid>
									</f:facet>
									<f:facet name="controls">
										<a4j:commandLink id="upd2"
											action="#{urgenceGestionBean.viderchamp}"
											reRender="accor1,accoruser1,accor2,accoruser2,buttonDelUpUser,buttonValidateUser,Label,data,scroller,record">
											<h:graphicImage value="../framGraphique/images/close.png"
												width="15" height="15" />
										</a4j:commandLink>
									</f:facet>
									<h:form>
										<table align="center">
											<tr>
												<td align="left"><h:graphicImage
														value="../framGraphique/images/information.png" /></td>
												<td align="center" colspan="2"><h:outputText
														styleClass="xtePopupInformationText"
														value="#{urgenceGestionBean.message}" /></td>
											</tr>
										</table>
										<div align="center">
											<a4j:commandButton value="#{msg.buttonOk}"
												action="#{urgenceGestionBean.viderchamp}"
												oncomplete="Richfaces.hideModalPanel('msgPanal');"
												type="submit" ajaxSingle="true" styleClass="submitButton"
												reRender="accor1,accoruser1,accor2,accoruser2,buttonDelUpUser,buttonValidateUser,Label,data,scroller,record" />
										</div>
									</h:form>
								</rich:modalPanel>
								<!-- *****popup Erreur de modification****** -->
								<rich:modalPanel id="msgPanalErreur" autosized="true"
									height="90" width="350">
									<a4j:keepAlive beanName="urgenceGestionBean" />
									<f:facet name="header">
										<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
											<rich:spacer width="2" />
											<h:outputText value="#{msg.labelError}"
												styleClass="xtePopupInformationTextHeader" />
										</h:panelGrid>
									</f:facet>
									<f:facet name="controls">
										<h:panelGroup>
											<h:graphicImage value="../framGraphique/images/close.png"
												width="15" height="15" id="hidelinkErreur" />
											<rich:componentControl for="msgPanalErreur"
												attachTo="hidelinkErreur" operation="hide" event="onclick" />
										</h:panelGroup>
									</f:facet>
									<h:form>
										<table align="center">
											<tr>
												<td align="left"><h:graphicImage
														value="../framGraphique/images/erreur.png" /></td>
												<td align="center" colspan="2"><h:outputText
														styleClass="xtePopupInformationText"
														value="#{urgenceGestionBean.message}" /></td>
											</tr>
										</table>
										<div align="center">
											<a4j:commandButton value="#{msg.buttonOk}"
												onclick="Richfaces.hideModalPanel('msgPanalErreur');"
												type="submit" ajaxSingle="true" styleClass="submitButton" />
										</div>
									</h:form>
								</rich:modalPanel>
					</div>
					<rich:panel styleClass="rich-panel">
						<f:facet name="header">
							<h:panelGrid columns="3">
								<h:outputText value="#{msg.listUrgencefacet}"
									styleClass="xteEnregistrementLabelOutput" />
							</h:panelGrid>
						</f:facet>
						<a4j:keepAlive beanName="urgenceGestionBean" />
						<h:form>
							<rich:dataTable value="#{urgenceGestionBean.listUrgence}"
								var="urgence" id="data" rows="10" styleClass="rich-panel">
								<rich:column width="12%" sortBy="#{urgence.urgenceId}"
									filterBy="#{urgence.urgenceId}" filterEvent="onkeyup"
									label="ref">
									<f:facet name="header">
										<h:outputText value="#{msg.Reference}" />
									</f:facet>
									<div align="center">
										<h:outputText value="#{urgence.urgenceId}" />
									</div>

								</rich:column>

								<rich:column width="12%" sortBy="#{urgence.urgenceLibelle}"
									filterBy="#{urgence.urgenceLibelle}" filterEvent="onkeyup"
									label="label">
									<f:facet name="header">
										<h:outputText value="#{msg.libelle1}" />
									</f:facet>
									<h:outputText value="#{urgence.urgenceLibelle}" rendered="#{urgenceGestionBean.urgenceLibelleShowFr}"/>
									<h:outputText value="#{urgence.urgenceLibelleAr}" rendered="#{urgenceGestionBean.urgenceLibelleShowAr}"/>
								</rich:column>
								<rich:column sortBy="#{urgence.urgenceDescription}"
									filterBy="#{urgence.urgenceDescription}" filterEvent="onkeyup"
									label="label">
									<f:facet name="header">
										<h:outputText value="#{msg.comment}" />
									</f:facet>
									<h:outputText value="#{urgence.urgenceDescription}" rendered="#{urgenceGestionBean.urgenceDescriptionShowFr}"/>
									<h:outputText value="#{urgence.urgenceDescriptionAr}" rendered="#{urgenceGestionBean.urgenceDescriptionShowAr}"/>
								</rich:column>
								<rich:column width="10%">
									<f:facet name="header">
										<h:outputText value="#{msg.operation}" />
									</f:facet>
									<a4j:keepAlive beanName="urgenceGestionBean" />
									<center>
										<h:panelGrid columns="6">
											<a4j:commandLink id="ed"
												action="#{urgenceGestionBean.getSelectionRow}"
												reRender="accor1,accoruser1,accor2,accoruser2,buttonDelUpUser,buttonValidateUser,Label,
												btnAnnull,btnAnnul,btnAjout,btnModif,Identifiant,Nom,Categorie,Description,outputAjout">
												<h:graphicImage
													value="../framGraphique/images/icone-detail.png"
													alt="editer" />
											</a4j:commandLink>

											<!--											<rich:spacer width="5" style=" height : 2px;" />-->
											<rich:toolTip for="ed" value="#{msg.labelEdit}"
												direction="top-right" />
											<a4j:commandLink id="del"
												action="#{urgenceGestionBean.getSelectionRow}"
												reRender="accor1,accoruser1,accor2,accoruser2,buttonDelUpUser,buttonValidateUser,Label"
												oncomplete="#{rich:component('askDelete')}.show()">
												<h:graphicImage value="../framGraphique/images/Delete.png"
													alt="Delete" style=" width : 16px;" />
											</a4j:commandLink>

											<rich:toolTip for="del" value="#{msg.labelDelete}"
												direction="top-right" />
										</h:panelGrid>
									</center>
									<rich:toolTip for="upd" direction="top-right"
										value="#{msg.labelConsulter}" />
								</rich:column>
							</rich:dataTable>
							<div align="center">
								<rich:datascroller for="data" id="scroller" />
								<h:outputText value="#{msg.nbEnregistrement}"
									styleClass="xteNombreEnregistrementLabelOutput" />
								<h:outputText id="record" value="#{urgenceGestionBean.records}"
									styleClass="xteNombreEnregistrementLabelOutput" />
							</div>
						</h:form>
						<!-- *********ask Delete*********** -->
						<rich:modalPanel id="askDelete" autosized="true" height="90"
							width="350">
							<f:facet name="header">
								<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
									<h:outputText value="#{msg.labelConfirm}"
										styleClass="xtePopupInformationTextHeader" />
								</h:panelGrid>
							</f:facet>
							<f:facet name="controls">
								<h:panelGroup>
									<h:graphicImage value="../framGraphique/images/close.png"
										width="15" height="15" id="hidelink2" alt="Fermer" />
									<rich:componentControl for="askDelete" attachTo="hidelink2"
										operation="hide" event="onclick" />
								</h:panelGroup>
							</f:facet>
							<a4j:keepAlive beanName="urgenceGestionBean" />
							<h:form>
								<table align="center">
									<tr>
										<td align="left"><h:graphicImage
												value="../framGraphique/images/interrogation.png" /></td>
										<td align="center" colspan="2"><h:outputText
												styleClass="xtePopupInformationText"
												value="#{msg.confirmDelete}" /></td>
									</tr>
								</table>
								<div align="center">
									<a4j:commandButton value="#{msg.buttonOui}"
										action="#{urgenceGestionBean.deleteSelectedRow}"
										oncomplete="Richfaces.hideModalPanel('askDelete'); #{rich:component('msgPanalCD')}.show();"
										type="submit" ajaxSingle="true" styleClass="submitButton" />
									<rich:spacer width="10" />
									<a4j:commandButton value="#{msg.buttonNon}"
										styleClass="submitButton"
										onclick="Richfaces.hideModalPanel('askDelete');" />
								</div>
							</h:form>
						</rich:modalPanel>
						<!-- *****popup Info validation de suppression******* -->
						<rich:modalPanel id="msgPanalCD" autosized="true" height="90"
							width="350">
							<a4j:keepAlive beanName="urgenceGestionBean" ajaxOnly="true" />
							<f:facet name="header">
								<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
									<rich:spacer width="2" />
									<h:outputText value="#{msg.labelInfo}"
										styleClass="xtePopupInformationTextHeader" />
								</h:panelGrid>
							</f:facet>
							<f:facet name="controls">
								<a4j:commandLink id="upd3"
									action="#{urgenceGestionBean.viderchamp}"
									reRender="accor1,accoruser1,accor2,accoruser2,buttonDelUpUser,buttonValidateUser,Label,data,scroller,record">
									<h:graphicImage value="../framGraphique/images/close.png"
										width="15" height="15" />
								</a4j:commandLink>
							</f:facet>
							<h:form>
								<table align="center">
									<tr>
										<td align="left"><h:graphicImage
												value="../framGraphique/images/information.png" /></td>
										<td align="center" colspan="2"><h:outputText
												styleClass="xtePopupInformationText"
												value="#{msg.confirmSuppression}" /></td>
									</tr>
								</table>
								<div align="center">
									<a4j:commandButton value="#{msg.buttonOk}"
										oncomplete="Richfaces.hideModalPanel('msgPanalCD');"
										type="submit" ajaxSingle="true" styleClass="submitButton"
										action="#{urgenceGestionBean.viderchamp}"
										reRender="accor1,accoruser1,accor2,accoruser2,buttonDelUpUser,buttonValidateUser,Label,data,scroller,record" />
								</div>
							</h:form>
						</rich:modalPanel>
					</rich:panel>
				</div>
				<div id="inter" />
				<div id="corpsDroite">

					<div class="monthenc">
						<script type="text/javascript">
//<![CDATA[
            document.write(monthi8[w4s.getMonth()]); //]]>
            </script>
					</div>
					<div class="datebx">
						<script type="text/javascript">
//<![CDATA[
            document.write(s6ix[s3x]); //]]>
            </script>
						<div class="dateenc">
							<a href="http://www.websiteadministrator.com.au/"><script
