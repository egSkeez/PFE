<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-
transitional.dtd">

<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:sec="http://www.springframework.org/security/tags">
	<html>
<head>
<title>Gestion unités/bureaux d'ordes</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<script src="../framGraphique/#{languageManager.calendar}"
	type="text/javascript"></script>
</head>
<body>
	<f:view locale="#{languageManager.locale}">
		<link rel="STYLESHEET" type="text/css"
			href="../framGraphique/#{languageManager.css}" title="css"></link>
		<div id="page">
			<div id="header">
				<ui:include src="../framGraphique/header.xhtml"></ui:include>
			</div>
			<div id="menuPrincipale">
				<ui:include src="../framGraphique/toolbar.xhtml"></ui:include>
			</div>
			<div id="contenu" style="height: auto;">
				<div style="width: 1200px;">
					<div id="corpsGaucheUnite">
						<rich:panel styleClass="rich-panel">
							
							<rich:modalPanel id="aidePanal" autosized="true" height="500"
								width="800">
								<f:facet name="header">
									<h:panelGrid columns="3" cellpadding="0" cellspacing="0">

										<h:graphicImage value="../framGraphique/images/help_16.png"
											id="help" />
										<h:outputText
											value="#{msg.uniteBocGestionAide} #{aideEnLigneBean.titreInterface}"
											styleClass="xtePopupInformationTextHeader" />
									</h:panelGrid>
								</f:facet>
								<f:facet name="controls">
									<h:panelGroup>
										<h:graphicImage value="../framGraphique/images/close.png"
											width="15" height="15" id="aidehidelink" />
										<rich:componentControl for="aidePanal" attachTo="aidehidelink"
											operation="hide" event="onclick" />
									</h:panelGroup>
								</f:facet>
								<div class="scroll_div">
									<h:form>
										<a4j:keepAlive beanName="aideEnLigneBean" />
										<h:graphicImage
											rendered="#{variableGlobale.locale eq 'fr_FR'}"
											value="../framGraphique/images/AideEnLigne/GBO-Gestion-Boc.jpg"
											width="750" height="490" id="aidehidelink" />
										<h:graphicImage rendered="#{variableGlobale.locale eq 'ar'}"
											value="../framGraphique/images/AideEnLigne/GBO-Unit-Boc-Gestion-AR.png"
											width="750" height="490" id="aidehidelinkAR" />
										<rich:spacer height="10" />
										<table align="center">
											<tr>
												<td align="center"><a4j:commandButton
														value="#{msg.bouttonOk}"
														oncomplete="Richfaces.hideModalPanel('aidePanal');"
														type="submit" ajaxSingle="true" styleClass="submitButton" />
												</td>
											</tr>
										</table>
									</h:form>
								</div>
							</rich:modalPanel>
						</rich:panel>
						<div>
							<h:form>
								<a4j:keepAlive beanName="uniteBocGestionBean" />
								<rich:simpleTogglePanel id="aco" styleClass="rich-panel"
									switchType="client" opened="true">
									<f:facet name="header">
										<h:panelGrid columns="2" style="margin:-3px;">
											<h:outputText value="#{msg.structureHierarchiquePrincipale}"
												styleClass="xteEnregistrementLabelOutput" />
										</h:panelGrid>
									</f:facet>

									<rich:tree value="#{uniteBocGestionBean.stationNodesPrincipal}"
										var="station"
										reRender="form1,form2,form3,form4,form5,form6,form7,form8,form9,form10,form11,tree"
										id="treePrincipal" ajaxSingle="true"
										nodeSelectListener="#{uniteBocGestionBean.getSelectedNodeForPrincipalTree}"
										icon="../framGraphique/images/dep.png"
										ajaxSubmitSelection="true" switchType="client">

										<rich:treeNode
											iconCollapsed="../framGraphique/images/hc_Addd.gif"
											iconExpanded="../framGraphique/images/hc_Add11.png"
											iconLeaf="../framGraphique/images/dep.png">
											<h:outputText value="#{station}" style="color:#666666;" />

										</rich:treeNode>
									</rich:tree>


								</rich:simpleTogglePanel>
							</h:form>
						</div>
						<div style="margin-top: -15px;">
							<h:form id="form11"
								style=" display: #{uniteBocGestionBean.showHierarchicStructureForm};">
								<div>
									<rich:panel>
										<f:facet name="header">
											<h:outputText value="#{msg.structureHierarchique}"
												styleClass="xtePopupInformationTextHeaderBo" />
										</f:facet>
										<rich:tree value="#{uniteBocGestionBean.stationNodes}"
											var="station"
											reRender="form1,form2,form3,form4,form5,form6,form7,form8,form9,dataDetailsUnit,scrollerDetailsUnit,recordDetailsUnit,dataDetailsBOC,scrollerDetailsBOC,recordDetailsBOC,tree"
											id="tree" ajaxSingle="true"
											style="width:auto;height:400px;overflow-x:scroll ; overflow-y: scroll;"
											nodeSelectListener="#{uniteBocGestionBean.getSelectedNode}"
											icon="../framGraphique/images/dep.png"
											ajaxSubmitSelection="true" switchType="client">

											<rich:treeNode
												iconCollapsed="../framGraphique/images/hc_Addd.gif"
												iconExpanded="../framGraphique/images/hc_Add11.png"
												iconLeaf="../framGraphique/images/dep.png">
												<h:outputText value="#{station}" style="color:#666666;" />

											</rich:treeNode>
										</rich:tree>
									</rich:panel>
								</div>
							</h:form>
						</div>
					</div>
					<div id="corpsDroiteUnite">

						<div style="width: 100%;">
							<div>
								<h:form id="form1"
									style=" display: #{uniteBocGestionBean.showUnitPanelForm};">
									<rich:panel styleClass="rich-panel">
										<h:panelGrid columns="12">
											<rich:spacer width="10px" />
											<h:column>
												<a4j:commandLink id="add"
													action="#{uniteBocGestionBean.loadAddUnitInterface}"
													reRender="form1,form2,form3,form4,form5,form6,form7,form8,form9,tree">
													<h:graphicImage id="add1"
														value="../framGraphique/images/icone-add.png"
														onmouseover="this.src='../framGraphique/images/icone-addMouseOver.png'"
														onmouseout="this.src='../framGraphique/images/icone-add.png'" />
												</a4j:commandLink>
												<rich:toolTip for="add1" direction="top-right"
													value="#{msg.labelAjoutBO}" />
											</h:column>
											<rich:spacer width="10px" />
											<h:column>
												<a4j:commandLink id="delete"
													oncomplete="if(#{uniteBocGestionBean.status1}==true) #{rich:component('askDelete1')}.show(); if(#{uniteBocGestionBean.status2}==true) #{rich:component('askDelete2')}.show(); if(#{uniteBocGestionBean.status3}==true) #{rich:component('askDelete3')}.show();"
													action="#{uniteBocGestionBean.getDeleteUnit}"
													reRender="askDelete1,askDelete2,askDelete3">
													<h:graphicImage id="delete1"
														value="../framGraphique/images/icone-delete.png"
														onmouseover="this.src='../framGraphique/images/icone-deleteMouseOver.png'"
														onmouseout="this.src='../framGraphique/images/icone-delete.png'" />
												</a4j:commandLink>
												<rich:toolTip for="delete" direction="top-right"
													value="#{msg.labelDeleteUnit}" />
											</h:column>
											<rich:spacer width="10px" />
											<h:column>
												<a4j:commandLink id="update"
													action="#{uniteBocGestionBean.loadUpdateUnitInterface}"
													rendered="#{uniteBocGestionBean.showModif}"
													reRender="form1,form2,form3,form4,form5,form6,form7,form8,form9,tree">
													<h:graphicImage id="update1"
														value="../framGraphique/images/edit_barreMenu.png"
														onmouseover="this.src='../framGraphique/images/edit_barreMenuMouseOver.png'"
														onmouseout="this.src='../framGraphique/images/edit_barreMenu.png'" />
												</a4j:commandLink>
												<rich:toolTip for="update" direction="top-right"
													value="#{msg.labelUpdateUnit}" />
											</h:column>
											<rich:spacer width="10px" />
											<h:column>
												<a4j:commandLink id="affect"
													action="#{uniteBocGestionBean.loadAffectUnitInterface}"
													reRender="form1,form2,form3,form4,form5,form6,form7,form8,form9,tree,menu,showControl">
													<h:graphicImage id="affect1"
														value="../framGraphique/images/mem.png" />
												</a4j:commandLink>
												<rich:toolTip for="affect" direction="top-right"
													value="#{msg.labelAffectUnit}" />
											</h:column>
											<rich:spacer width="485"></rich:spacer>
											<h:column>
												<a4j:commandLink id="helpButton"
													action="#{aideEnLigneBean.resultat}"
													oncomplete="#{rich:component('aidePanal')}.show();"
													reRender="aidePanal">
													<h:graphicImage id="j"
														value="../framGraphique/images/help.png" />
												</a4j:commandLink>
												<rich:toolTip for="helpButton" value="#{msg.help}" />
											</h:column>

										</h:panelGrid>
										<a4j:keepAlive beanName="uniteBocGestionBean" />
									</rich:panel>
								</h:form>
								<!-- ******popup delete 1****** -->
								<rich:modalPanel id="askDelete1" autosized="true" height="90"
									width="320">
									<a4j:keepAlive beanName="uniteBocGestionBean" ajaxOnly="true" />
									<f:facet name="header">
										<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
											<h:outputText value="#{msg.labelConfirm}"
												styleClass="xtePopupInformationTextHeader" />
										</h:panelGrid>
									</f:facet>
									<f:facet name="controls">
										<h:panelGroup>
											<h:graphicImage value="../framGraphique/images/close.png"
												width="15" height="15" id="hidelinkdel" alt="Fermer" />
											<rich:componentControl for="askDelete1"
												attachTo="hidelinkdel" operation="hide" event="onclick" />
										</h:panelGroup>
									</f:facet>
									<table align="center">
										<tr>
											<td align="left"><h:graphicImage
													value="../framGraphique/images/interrogation.png" /></td>
											<td align="center" colspan="2"><h:outputText
													styleClass="xtePopupInformationText"
													value="#{uniteBocGestionBean.message}" /></td>
										</tr>
									</table>
									<h:form>
										<div align="center">
											<a4j:commandButton value="#{msg.buttonOui}"
												action="#{uniteBocGestionBean.deleteUnit}"
												oncomplete="Richfaces.hideModalPanel('askDelete1'); #{rich:component('msgPanalCD')}.show();"
												type="submit" ajaxSingle="true" styleClass="submitButton" />
											<rich:spacer width="10" />
											<a4j:commandButton value="#{msg.buttonNon}"
												styleClass="submitButton"
												onclick="Richfaces.hideModalPanel('askDelete1');" />
										</div>
									</h:form>
								</rich:modalPanel>
								<!-- ******popup delete 2****** -->
								<rich:modalPanel id="askDelete2" autosized="true" height="110"
									width="350">
									<a4j:keepAlive beanName="uniteBocGestionBean" ajaxOnly="true" />
									<f:facet name="header">
										<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
											<h:outputText value="#{msg.labelAlerte}"
												styleClass="xtePopupInformationTextHeader" />
										</h:panelGrid>
									</f:facet>
									<f:facet name="controls">
										<h:panelGroup>
											<h:graphicImage value="../framGraphique/images/close.png"
												width="15" height="15" id="hidelinkdel1" alt="Fermer" />
											<rich:componentControl for="askDelete2"
												attachTo="hidelinkdel1" operation="hide" event="onclick" />
										</h:panelGroup>
									</f:facet>
									<table align="center">
										<tr>
											<td align="left"><h:graphicImage
													value="../framGraphique/images/avertissement.png" /></td>
											<td align="center" colspan="2"><h:outputText
													styleClass="xtePopupInformationText"
													value="#{uniteBocGestionBean.message}" /></td>
										</tr>
									</table>
									<h:form>
										<div align="center">
											<a4j:commandButton value="#{msg.buttonOk}"
												action="#{uniteBocGestionBean.loadAffectUnitInterface}"
												reRender="form1,form2,form3,form4,form5,form6,form7,form8,form9,tree"
												oncomplete="Richfaces.hideModalPanel('askDelete2'); "
												type="submit" ajaxSingle="true" styleClass="submitButton" />
											<rich:spacer width="10" />
										</div>
									</h:form>
								</rich:modalPanel>
								<!-- ******popup delete 3****** -->
								<rich:modalPanel id="askDelete3" autosized="true" height="110"
									width="410">
									<a4j:keepAlive beanName="uniteBocGestionBean" ajaxOnly="true" />
									<f:facet name="header">
										<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
											<h:outputText value="#{msg.labelAlerte}"
												styleClass="xtePopupInformationTextHeader" />
										</h:panelGrid>
									</f:facet>
									<f:facet name="controls">
										<h:panelGroup>
											<h:graphicImage value="../framGraphique/images/close.png"
												width="15" height="15" id="hidelinkdel3" alt="Fermer" />
											<rich:componentControl for="askDelete3"
												attachTo="hidelinkdel3" operation="hide" event="onclick" />
										</h:panelGroup>
									</f:facet>
									<table align="center">
										<tr>
											<td align="left"><h:graphicImage
													value="../framGraphique/images/avertissement.png" /></td>
											<td align="center" colspan="2"><h:outputText
													styleClass="xtePopupInformationText"
													value="#{uniteBocGestionBean.message}" />
											</td>
										</tr>
									</table>
									<h:form>
										<div align="center">
											<a4j:commandButton value="#{msg.buttonOk}"
												reRender="form1,form2,form3,form4,form5,form6,form7,form8,form9,tree"
												action="#{uniteBocGestionBean.firstInitializeAffectation}"
												oncomplete="Richfaces.hideModalPanel('askDelete3');"
												type="submit" ajaxSingle="true" styleClass="submitButton" />
										</div>
									</h:form>
								</rich:modalPanel>
								<!-- *****popup Info validation de suppression******* -->
								<rich:modalPanel id="msgPanalCD" autosized="true" height="90"
									width="350">
									<a4j:keepAlive beanName="uniteBocGestionBean" ajaxOnly="true" />
									<f:facet name="header">
										<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
											<rich:spacer width="2" />
											<h:outputText value="#{msg.labelInfo}"
												styleClass="xtePopupInformationTextHeader" />
										</h:panelGrid>
									</f:facet>
									<f:facet name="controls">
										<a4j:commandLink id="upd1"
											action="#{uniteBocGestionBean.afterDelete}">
											<h:graphicImage value="../framGraphique/images/close.png"
												width="15" height="15" />
										</a4j:commandLink>

									</f:facet>
									<h:form>
										<table align="center">
											<tr>
												<td align="left"><h:graphicImage
														value="../framGraphique/images/information.png" /></td>
												<td align="center" colspan="2"><h:outputText
														styleClass="xtePopupInformationText"
														value="#{msg.confirmSuppression}" /></td>
											</tr>
										</table>
										<div align="center">
											<a4j:commandButton value="#{msg.buttonOk}"
												oncomplete="Richfaces.hideModalPanel('msgPanalCD');"
												type="submit" ajaxSingle="true" styleClass="submitButton"
												action="#{uniteBocGestionBean.afterDelete}"
												reRender="form1,form2,form3,form4,form5,form6,form7,form8,form9,tree,treePrincipal" />
										</div>
									</h:form>
								</rich:modalPanel>
							</div>

							<div>
								<h:form id="form2"
									style="height:auto; display: #{uniteBocGestionBean.showDetailsUnitForm};">
									<div>
										<a4j:keepAlive beanName="uniteBocGestionBean" ajaxOnly="true" />
										<rich:panel styleClass="rich-panel">
											<f:facet name="header">
												<h:panelGrid columns="1" styleClass="xtePanelGrid">
													<h:outputText value="#{uniteBocGestionBean.unitTitle}"
														styleClass="xteEnregistrementLabelOutput" />
												</h:panelGrid>
											</f:facet>
											<table width="100%">
												<tr>
													<td width="50%" style="vertical-align: top;"><h:panelGrid
															columns="2">
															<h:column>
																<h:outputText
																	value="#{msg.reference}#{msg.pointvirgule}"
																	styleClass="xteDetailLabel" />
															</h:column>
															<h:column>
																<h:outputLabel
																	value="#{uniteBocGestionBean.unitReference}"
																	styleClass="xteDetailLabelOutput" />
															</h:column>
															<h:column>
																<h:outputText
																	value="#{uniteBocGestionBean.unitNameLabel}#{msg.pointvirgule}"
																	styleClass="xteDetailLabel" />
															</h:column>

															<h:column>
																<h:outputText value="#{uniteBocGestionBean.unitName}"
																	styleClass="xteDetailLabelOutput" />
															</h:column>
															<h:column>
																<h:outputText
																	value="#{msg.abreviation}#{msg.pointvirgule}"
																	styleClass="xteDetailLabel" />
															</h:column>
															<h:column>
																<h:outputLabel
																	value="#{uniteBocGestionBean.unitShortName}"
																	styleClass="xteDetailLabelOutput" />
															</h:column>



														</h:panelGrid></td>
													<td width="50%" valign="top"><h:panelGrid columns="2">
															<h:column  rendered="#{uniteBocGestionBean.affichageRattachement}">
																<h:outputText
																	value="#{msg.BORattachement}#{msg.pointvirgule}"
																	styleClass="xteDetailLabel" />
															</h:column>
															<h:column  rendered="#{uniteBocGestionBean.affichageRattachement}">
																<h:outputText
																	value="#{uniteBocGestionBean.associatedBoc}"
																	styleClass="xteDetailLabelOutput" />
															</h:column >
															<h:column rendered="#{uniteBocGestionBean.affichageRattachement}">
																<h:outputText
																	value="#{msg.uniteRattachement}#{msg.pointvirgule}"
																	styleClass="xteDetailLabel" />
															</h:column>
															<h:column  rendered="#{uniteBocGestionBean.affichageRattachement}">
																<h:outputText
																	value="#{uniteBocGestionBean.associatedUnit}"
																	styleClass="xteDetailLabelOutput" />
															</h:column>
															<h:column>
																<h:outputText value="#{msg.comment}#{msg.pointvirgule}"
																	styleClass="xteDetailLabel" style="width:160px;" />
															</h:column>
															<h:column>
																<h:outputLabel
																	value="#{uniteBocGestionBean.unitDescription}"
																	styleClass="xteDetailLabelOutput" />
															</h:column>
														</h:panelGrid></td>
												</tr>
											</table>

										</rich:panel>
									</div>
								</h:form>
							</div>

							<div>
								<h:form id="form3"
									style="height:auto; display: #{uniteBocGestionBean.showAddUnitForm};">
									<div>
										<a4j:keepAlive beanName="uniteBocGestionBean" ajaxOnly="true" />
										<rich:panel id="accor1" styleClass="rich-panel">
											<f:facet name="header">
												<h:panelGrid columns="3">
													<h:outputText value="#{uniteBocGestionBean.unitTitle}"
														styleClass="xteEnregistrementLabelOutput" />
												</h:panelGrid>
											</f:facet>
											<rich:messages ajaxRendered="true" errorClass="msgError"
												styleClass="error" />
												<div align="center">
														<h:panelGrid columns="2">
															<h:column>
																<h:outputText  rendered="#{not(uniteBocGestionBean.affichageListeRattachement)}"
																	value="#{msg.choixTypeUnite}#{msg.pointvirgule}"
																	styleClass="xteDetailLabel" />
																<rich:spacer width="5" />
															</h:column>
															<h:column>
																<h:selectOneMenu id="inputt" disabled="#{variableGlobale.sonede}" rendered="#{not(uniteBocGestionBean.affichageListeRattachement)}"
																	value="#{uniteBocGestionBean.selectedUnit}"
																	styleClass="xteDetailLabelcombox"
																	style=" width : 141px; height : 20px;">
																	<a4j:support event="onchange"
																		reRender="output1boc,checkUnit,content,outputTitle,form9,inputt1,inputt2boc,outputURboc"
																		ajaxSingle="true"
																		actionListener="#{uniteBocGestionBean.eventChooseUnitBoc}" />
																	<f:selectItems
																		value="#{uniteBocGestionBean.selectItemsT}" />
																</h:selectOneMenu>
															</h:column>
														</h:panelGrid>
													</div>
													
											<table width="100%">
												<tr>
													<td style="width: 50%; vertical-align: top;"><h:panelGrid
															columns="2">
															<h:column>
																<h:outputText id="outputURboc"
																	value="#{uniteBocGestionBean.unitNameLabel}#{msg.pointvirgule}"
																	styleClass="xteDetailLabel" />
																<rich:spacer width="5" />
																<h:outputText value="*" styleClass="error" />
															</h:column>
															<h:column>
																<h:inputText id="inputt1boc"
																	value="#{uniteBocGestionBean.unitName}" required="true"
																	requiredMessage="* #{msg.prenom} #{msg.champsobligatoire}"
																	maxlength="255">
