<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-
transitional.dtd">

<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich">
	<html>
<head>
<title>Consulter Courrier</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<script src="../framGraphique/#{languageManager.calendar}"
	type="text/javascript"></script>
</head>
<body>
	<f:view locale="#{languageManager.locale}">
		<link rel="STYLESHEET" type="text/css"
			href="../framGraphique/#{languageManager.css}" title="css"></link>
		<div id="page">
			<div id="header">
				<ui:include src="../framGraphique/header.xhtml"></ui:include>
			</div>
			<div id="menuPrincipale">
				<ui:include src="../framGraphique/toolbar.xhtml"></ui:include>
			</div>
			<div id="contenu">
				<div id="corpsGauche">
					<rich:panel styleClass="rich-panel" style="height:45px;">
						<h:form id="frmglobale" >
							<div id="barreIcone">
								<div id="barreIconeGauche" style="width:550px">
								<h:form id="fr1">
									<a4j:commandLink id="retour"
										action="#{courrierDetailsBean.backwardPage}">
										<h:graphicImage value="../framGraphique/images/retour.png"
											onclick="this.src='../framGraphique/images/retourClick.png'"
											onmouseover="this.src='../framGraphique/images/retourMouseOver.png'"
											onmouseout="this.src='../framGraphique/images/retour.png'" />
									</a4j:commandLink>
									<rich:toolTip for="retour" value="#{msg.Retour}"
										direction="top-right" />
									<rich:spacer width="15" />
									<a4j:commandLink id="modif"
										action="#{courrierDetailsBean.modify}" title="Modifier"
										reRender="askDelete2"
										oncomplete="if(#{courrierDetailsBean.status}==true) #{rich:component('askDelete2')}.show(); else document.location.href='courrierModifier.jsf'">
										<h:graphicImage
										rendered="#{courrierDetailsBean.showModificationButton}"
											value="../framGraphique/images/edit_barreMenu.png"
											onmouseover="this.src='../framGraphique/images/edit_barreMenuMouseOver.png'"
											onmouseout="this.src='../framGraphique/images/edit_barreMenu.png'"
											alt="Update" />
									</a4j:commandLink>
									<rich:toolTip for="modif" value="#{msg.Modif}"
										direction="top-right" />
									
									<!-- 	<a4j:commandLink id="deletelink"
										onclick="#{rich:component('askDelete1')}.show(); ">
										<h:graphicImage
											value="../framGraphique/images/icone-delete.png"
											onmouseover="this.src='../framGraphique/images/icone-deleteMouseOver.png'"
											onmouseout="this.src='../framGraphique/images/icone-delete.png'"
											alt="Delete" />
									</a4j:commandLink>  -->
									<rich:toolTip for="deletelink" value="#{msg.Supprim}"
										direction="top-right" />
									<rich:spacer width="3" />
								<a4j:commandLink id="ajout" status="ajaxStatusWait" 
									action="#{courrierDetailsBean.modify}"
									reRender="askDelete2,askAjoutDoc"
										oncomplete="if(#{courrierDetailsBean.status4}==true) #{rich:component('askAjoutDoc')}.show(); else document.location.href='documentAjout.jsf'"
										>
										<h:graphicImage value="../framGraphique/images/icone-add.png"										
											onmouseover="this.src='../framGraphique/images/icone-addMouseOver.png'"
											onmouseout="this.src='../framGraphique/images/icone-add.png'"
											alt="ajouter" rendered="#{variableGlobale.gedStatus==true and courrierDetailsBean.showAjoutDocumentButton==true}"/>
									</a4j:commandLink>
									<rich:toolTip for="ajout" value="#{msg.AjouterDocument}"
										direction="top-right" />
									<rich:spacer width="5" />
									<a4j:commandLink id="suivi"
										onclick="document.location.href='courrierSuiviDestinataire.jsf'">
										<h:graphicImage 
											rendered="#{variableGlobale.rendered}"
											value="../framGraphique/images/suivi.png"
											onmouseover="this.src='../framGraphique/images/suiviMouseOver.png'"
											onmouseout="this.src='../framGraphique/images/suivi.png'"
											alt="suivi" />
									</a4j:commandLink>
									<rich:toolTip for="suivi" value="#{msg.Suivi}"
										direction="top-right" />
									<rich:spacer width="5" />					
									<a4j:commandLink id="Transfer"
									rendered="#{not(variableGlobale.person.boc)}"
										action="#{courrierDetailsBean.transfert}"
										oncomplete="document.location.href='courrierTransfere.jsf'">
										<h:graphicImage
											rendered="true"
											value="../framGraphique/images/TransfereIconBig.png"
											alt="suivi" />
									</a4j:commandLink>
									<rich:toolTip for="Transfer" value="#{msg.FacetTransfer}"
										direction="top-right" />
									<rich:spacer width="5" />
									<a4j:commandLink id="repondre"
										rendered="#{variableGlobale.rendered}"
										action="#{courrierDetailsBean.reply}"
										oncomplete="document.location.href='courrierRepondre.jsf'">
<!--										rendered="#{courrierDetailsBean.showResponseButton}"-->
										<h:graphicImage
											rendered="#{courrierDetailsBean.showResponseButton}"
											value="../framGraphique/images/RepondreIconBig.png" />
											
										<!--											 onmouseover="this.src='../framGraphique/images/repondreMouseOver.png'"-->
										<!--											onmouseout="this.src='../framGraphique/images/repondre.png'"-->
									</a4j:commandLink>
									<rich:toolTip for="repondre" value="#{msg.Reponse}" />
									<rich:spacer width="5" />
									<h:graphicImage id="repondre1"
										rendered="#{courrierDetailsBean.hideResponseButton}"
										value="../framGraphique/images/RepondreIconBigDisab.png"/>
<!--										rendered="#{courrierDetailsBean.hideResponseButton}" -->
									<rich:toolTip for="repondre1" value="#{msg.repondre}"
										direction="top-right" />
									<rich:spacer width="5" />
									<a4j:commandLink id="aValider" disabled="#{!courrierDetailsBean.showForValidate}"
															 ajaxSingle="true" rendered="#{variableGlobale.person.responsable and not(variableGlobale.person.boc)}"
															onclick="document.location.href='courrierValidation.jsf'">
															<h:graphicImage rendered="#{courrierDetailsBean.showForValidate}"
																value="../framGraphique/images/courrierAValiderBig.png"
																alt="aValider" />
																<rich:spacer width="2" />
																<h:graphicImage rendered="#{!courrierDetailsBean.showForValidate}"
															value="../framGraphique/images/courrierAValiderDisabledBig.png"
															alt="aValiderDisabled" />
													
															
														</a4j:commandLink>
									<rich:toolTip for="aValider" value="#{msg.aValider}"
										disabled="#{!courrierDetailsBean.showForValidate}" />
									

									<rich:spacer width="5" />
									<a4j:commandLink id="classement"
										rendered="#{variableGlobale.person.boc or variableGlobale.person.responsable or variableGlobale.person.secretary}"
										action="#{courrierDetailsBean.classement}"
										oncomplete="if(#{courrierDetailsBean.statusNonClasse}==true) #{rich:component('confirmation')}.show(); if(#{courrierDetailsBean.statusClassement}==true) #{rich:component('infoClassement')}.show();"
										reRender="confirmation,infoClassement">
										<h:graphicImage value="../framGraphique/images/archive.png"
											alt="infoClassement" rendered="#{variableGlobale.rendered}"/>
									</a4j:commandLink>
									<rich:toolTip for="classement" value="#{msg.Classement}" />
									<rich:spacer width="5" />
									<a4j:commandLink id="accuseReception"
										action="#{courrierDetailsBean.accesAccuseReception}"
										oncomplete="if(#{courrierDetailsBean.statusAccuseReception}==true) #{rich:component('askAccuseReception')}.show();"
										rendered="#{courrierDetailsBean.accuseReception}"
										reRender="askAccuseReception">
										<h:graphicImage
											rendered="#{variableGlobale.rendered}"
											value="../framGraphique/images/accuseReception.png"
											alt="infoClassement" />
									</a4j:commandLink>
									<rich:toolTip for="accuseReception" value="#{msg.reception}"
										direction="top-right" />
										<rich:spacer width="5" />
									<a4j:commandLink id="CopyCourrier" rendered="false"
										action="#{courrierCopy.copier}"
										oncomplete="if(#{courrierCopy.status}==true)
									 #{rich:component('msgPanal')}.show(); 
									 if(#{courrierCopy.status1}==true)
									  #{rich:component('msgPanal2')}.show();"
														reRender="msgPanal,msgPanal2">
										
										<h:graphicImage value="../framGraphique/images/copy.png"
											alt="infoClassement" rendered="#{variableGlobale.rendered}"/>
									</a4j:commandLink>
									
									<a4j:commandLink
										rendered="#{courrierDetailsBean.statusCloturer}"
										id="clot"
										oncomplete="#{rich:component('cloturer')}.show(); "
										reRender="cloturer">

										<h:graphicImage value="../framGraphique/images/switch_off.png"
											alt="Cloturé" rendered="#{variableGlobale.rendered}"/>
									</a4j:commandLink>
									
									<rich:toolTip rendered="#{courrierDetailsBean.statusCloturer}"
										for="clot" value="Clôturer" direction="top-right" />
									<a4j:commandLink rendered="#{courrierDetailsBean.statusActive}"
										id="react"
										oncomplete="#{rich:component('reactiver')}.show(); "
										reRender="reactiver">
										<h:graphicImage value="../framGraphique/images/switch_on.png"
											alt="Réactiver" rendered="#{variableGlobale.rendered}"/>
									</a4j:commandLink>
									<rich:toolTip for="react" value="Réactiver"
										direction="top-right" />
									
									<!-- 2019-07-24 Reception physique  -->
									<a4j:commandLink id="receptionPhysique" action="#{documentConsultationBean.goToReceptionPhysique}"
									                 oncomplete="document.location.href='courrierDetailsReceptionPhysique.jsf'" >
															<h:graphicImage rendered="#{courrierDetailsBean.etatReceptionPhysique}"
															value="../framGraphique/images/courrierPhy.png"
															alt="receptionPhysique" style=" width : 24px;"/>
															
									</a4j:commandLink>
									 
                                               <a4j:commandLink id="aValiderBoc"
															rendered="#{courrierDetailsBean.ci.courrierAValider eq 1}"
															action="#{courrierDetailsBean.executeJour}"
															ajaxSingle="true"
															oncomplete="if(#{courrierDetailsBean.statV1}==true)
																			#{rich:component('msgPanal1')}.show();
																		if(#{courrierDetailsBean.receptionphysiqueNonLivre}==true)
																			#{rich:component('msgPanalReceptionPhysique')}.show();
																		
																		if(#{courrierDetailsBean.statV2}==true)
																			#{rich:component('msgPanal2')}.show();"
															reRender="msgPanal1,msgPanal2,msgPanalReceptionPhysique">
															 
															<h:graphicImage
																value="../framGraphique/images/executeIcon.png"
																alt="aValider1" />
														</a4j:commandLink>
														<rich:toolTip for="aValiderBoc" value="#{msg.aExecuter}" />
														<h:graphicImage rendered="#{courrierDetailsBean.ci.courrierAValider eq 0}"
															value="../framGraphique/images/executeIconGrised.png"
															alt="aValiderDisabled1" />
														
														     
								<rich:modalPanel id="msgPanal1" autosized="true" height="100"
								width="400">
								<f:facet name="header">
									<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
										<rich:spacer width="2" />

										<h:outputText value="#{msg.labelInfo}"
											styleClass="popupInformationTextHeader" />
									</h:panelGrid>
								</f:facet>
								<f:facet name="controls">
									<h:panelGroup>
										<h:graphicImage value="../framGraphique/images/close.png"
											width="15" height="15" id="hidelink1" />
										<rich:componentControl for="msgPanal1" attachTo="hidelink1"
											operation="hide" event="onclick" />
									</h:panelGroup>
								</f:facet>
								<h:form>
									<a4j:keepAlive beanName="courrierDetailsBean" />
									<table align="center">
										<tr>
											<td align="left"><h:graphicImage
													value="../framGraphique/images/interrogation.png"
													alt="confirm" />
											</td>
											<td align="center" colspan="2"><h:outputText
													styleClass="popupInformationText"
													value="#{msg.CourrierTransferMsg}" />
											</td>
										</tr>
									</table>
									<table align="center">
										<tr>
											<td align="center">
											<a4j:commandButton
													value="#{msg.bouttonOk}"
													onclick="Richfaces.hideModalPanel('msgPanal1');"
													reRender="frmglobale" ajaxSingle="true"
													type="submit" styleClass="submitButton" />
											</td>
										</tr>
									</table>
								</h:form>
							</rich:modalPanel>

							<rich:modalPanel id="msgPanal2" autosized="true" height="100"
								width="400">
								<a4j:keepAlive beanName="courrierConsultationJourBean" />
								<f:facet name="header">
									<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
										<rich:spacer width="2" />
										<h:outputText value="#{msg.labelError}"
											styleClass="popupInformationTextHeader" />
									</h:panelGrid>
								</f:facet>
								<f:facet name="controls">
									<h:panelGroup>
										<h:graphicImage value="../framGraphique/images/close.png"
											width="15" height="15" id="hidelink2" />
										<rich:componentControl for="msgPanal2" attachTo="hidelink2"
											operation="hide" event="onclick" />
									</h:panelGroup>
								</f:facet>
								<h:form>
									<table align="center">
										<tr>
											<td align="left"><h:graphicImage
													value="../framGraphique/images/erreurrrrr.png"
													alt="confirm" />
											</td>
											<td align="center" colspan="2"><h:outputText
													styleClass="popupInformationText"
													value="#{msg.erreurTransfert}" />
											</td>
										</tr>
									</table>
									<table align="center">
										<tr>
											<td align="center"><a4j:commandButton
													value="#{msg.bouttonOk}"
													onclick="Richfaces.hideModalPanel('msgPanal2')"
													type="submit" ajaxSingle="true" styleClass="submitButton" />
											</td>
										</tr>
									</table>
								</h:form>
							</rich:modalPanel>
							</h:form>
								</div>
								<div id="barreIconeDroite" style="width:230px">
									<h:commandLink id="BordereauCourrier"
										action="#{rapportBordereauEnvoiInterneCourrier.printPDF}"
										rendered="#{courrierDetailsBean.boutonBordereau}">
										<h:graphicImage
											value="../framGraphique/images/BordereauCourrier.png"
											alt="BordereauCourrier" />
									</h:commandLink>
									<rich:toolTip for="BordereauCourrier"
										value="#{msg.BordereauCourrier}" />
									<h:commandLink id="pdf"
										action="#{rapportCourrierFiche.printPDFFR}">
										<h:graphicImage value="../framGraphique/images/PDF.png"
											onmouseover="this.src='../framGraphique/images/PDF.png'"
											onmouseout="this.src='../framGraphique/images/PDF.png'"
											alt="pdf" />
									</h:commandLink>
									<h:commandLink id="notetr"
										action="#{rapportCourrierFiche.printPDFNoteTransmission}">
										<h:graphicImage value="../framGraphique/images/PDF.png"
											onmouseover="this.src='../framGraphique/images/PDF.png'"
											onmouseout="this.src='../framGraphique/images/PDF.png'"
											alt="pdf" />
									</h:commandLink>
									<rich:toolTip for="notetr" value="#{msg.noteTransmission}"
										direction="top-right" />
									<h:commandLink  id="kha"
										action="#{rapportCourrierFiche.printPDFBordereauEnvoiMinistre}"
										status="ajaxStatusRapport">
										<h:graphicImage width="28" height="25"  value="../framGraphique/images/courrierInterne.jpg"
											onmouseover="this.src='../framGraphique/images/courrierInterne.jpg'"
											onmouseout="this.src='../framGraphique/images/courrierInterne.jpg'"
											alt="pdf" rendered="#{variableGlobale.rendered}"/>
									</h:commandLink>
									<rich:toolTip for="kha" value="#{msg.borMinistre}"
										direction="top-right" />
									
									<h:commandLink id="dech" status="ajaxStatusRapport"
										action="#{rapportCourrierFiche.printPDFDech}"
										rendered="#{courrierDetailsBean.showRapp} and  #{variableGlobale.person.boc}">
										<h:graphicImage width="23" height="25"
											value="../framGraphique/images/Stamp.png"
											onmouseover="this.src='../framGraphique/images/Stamp.png'"
											onmouseout="this.src='../framGraphique/images/Stamp.png'"
											alt="pdf" />
									</h:commandLink>
									<h:commandLink id="dechar" status="ajaxStatusRapport"
									rendered="#{courrierDetailsBean.showRapp} and  #{variableGlobale.person.boc}"
										action="#{rapportCourrierFiche.printPDFDechargeCourrierAR}">
										<h:graphicImage width="23" height="25"
											value="../framGraphique/images/Stamp.png"
											onmouseover="this.src='../framGraphique/images/Stamp.png'"
											onmouseout="this.src='../framGraphique/images/Stamp.png'"
											alt="pdf4" />
									</h:commandLink>
									<rich:toolTip for="dechar" value="#{msg.pdfExportDechargeAR}"
										direction="top-right" />
									<h:commandLink id="img2"
										action="#{rapportCourrierFiche.printImage}"
										status="ajaxStatusRapport">
										<h:graphicImage width="28" height="25"
											value="../framGraphique/images/image.png"
											onmouseover="this.src='../framGraphique/images/image.png'"
											onmouseout="this.src='../framGraphique/images/image.png'"
											alt="image" 
											rendered="false"/>
									</h:commandLink>
									<rich:toolTip for="pdf" value="#{msg.pdfExportDetails}"
										direction="top-right" />
									<rich:toolTip for="img2" value="#{msg.imgExportDetails}"
										direction="top-right" />
									<rich:toolTip for="dech" value="#{msg.pdfExportDecharge}"
										direction="top-right" />
									<h:commandLink id="pdf2"
									rendered="#{variableGlobale.person.boc}"
										action="#{rapportCourrierReference.printPDFFRDirect}"
										target="_blank" width="23" height="20">
										<h:graphicImage value="../framGraphique/images/compostage.png"
											onmouseover="this.src='../framGraphique/images/compostage.png'"
											onmouseout="this.src='../framGraphique/images/compostage.png'"
											alt="pdf2" />
									</h:commandLink>
									<rich:toolTip for="pdf2" value="#{msg.compostage}"
										direction="top-right" />

									<h:commandLink id="word" rendered ="#{not(variableGlobale.sonede)}"
										action="#{rapportCourrierFiche.printWORDFR}">
										<h:graphicImage value="../framGraphique/images/WORD.png"
											onmouseover="this.src='../framGraphique/images/WORD.png'"
											onmouseout="this.src='../framGraphique/images/WORD.png'"
											alt="word" 
											rendered="#{variableGlobale.rendered}"/>
									</h:commandLink>
									<rich:toolTip for="word" value="#{msg.wordExport}"
										direction="top-right" />

									<a4j:commandLink id="helpButton"
										action="#{aideEnLigneBean.resultat}"
										oncomplete="#{rich:component('aidePanal')}.show();"
										reRender="aidePanal">
										<h:graphicImage id="j"
											value="../framGraphique/images/help.png" />
									</a4j:commandLink>
									<rich:toolTip for="helpButton" value="#{msg.help}" />


								</div>
							</div>
						</h:form>
						<rich:modalPanel id="askDelete1" autosized="true" height="100"
							width="350">
							<a4j:keepAlive beanName="documentConsultationBean" />
							<f:facet name="header">
								<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
									<rich:spacer width="2" />
									<h:outputText value="#{msg.labelConfirmation}"
										styleClass="xtePopupInformationTextHeader" />
								</h:panelGrid>
							</f:facet>
							<f:facet name="controls">
								<h:panelGroup>
									<h:graphicImage value="../framGraphique/images/close.png"
										width="15" height="15" id="hidelink222" />
									<rich:componentControl for="askDelete1" attachTo="hidelink222"
										operation="hide" event="onclick" />
								</h:panelGroup>
							</f:facet>

							<table align="center">
								<tr>
									<td align="left"><h:graphicImage
											value="../framGraphique/images/information.png" alt="confirm" />
									</td>
									<td align="center" colspan="2"><h:outputText
											styleClass="xtePopupInformationText"
											value="Ce courrier est déjà envoyé, vous ne pouvez pas le supprimer." />
									</td>
								</tr>
							</table>
							<h:form>
								<div align="center">
									<a4j:commandButton value="#{msg.buttonOk}"
										onclick="Richfaces.hideModalPanel('askDelete1');"
										type="submit" ajaxSingle="true" styleClass="submitButton" />
								</div>
							</h:form>
						</rich:modalPanel>
						<rich:modalPanel id="askDelete2" autosized="true" height="110"
							width="390">
							<a4j:keepAlive beanName="courrierDetailsBean" ajaxOnly="true" />
							<f:facet name="header">
								<h:panelGrid columns="3" cellpadding="0" cellspacing="0">
									<h:outputText value="#{msg.labelAlerte}"
										styleClass="xtePopupInformationTextHeader" />
								</h:panelGrid>
							</f:facet>
							<f:facet name="controls">
								<h:panelGroup>
									<h:graphicImage value="../framGraphique/images/close.png"
										width="15" height="15" id="hidelinkdel1" alt="Fermer" />
									<rich:componentControl for="askDelete2" attachTo="hidelinkdel1"
										operation="hide" event="onclick" />
								</h:panelGroup>
