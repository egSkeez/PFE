				courrier.setCourrierTypeOrdre(1);
			} else {
				courrier.setCourrierTypeOrdre(lastId + 1);
			}
			courrier.setCourrierType("A");
			courrier.setCourrierReferenceCorrespondant(courrier
					.getCourrierType() + courrier.getCourrierTypeOrdre());
