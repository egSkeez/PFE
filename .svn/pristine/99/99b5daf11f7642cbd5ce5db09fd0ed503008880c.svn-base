package xtensus.ldap.model;

import java.util.List;

public class BOS implements java.io.Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -8053208561426187103L;
	private int idBOS;
	private String nameBOS;
	private List<Direction> listDirectionsChildBOS;
	private List<Person> membersBOS;
	private BOC associatedBOC;
	private int niveauBOS;
	private String descriptionBOS;
	private String rowKeyBOS;

	public BOS() {

	}

	public BOS(int idBOS, String nameBOS,
			List<Direction> listDirectionsChildBOS, List<Person> membersBOS,
			BOC associatedBOC, int niveauBOS) {
		this.idBOS = idBOS;
		this.nameBOS = nameBOS;
		this.listDirectionsChildBOS = listDirectionsChildBOS;
		this.membersBOS = membersBOS;
		this.associatedBOC = associatedBOC;
		this.setNiveauBOS(niveauBOS);
	}

	public int getIdBOS() {
		return idBOS;
	}

	public void setIdBOS(int idBOS) {
		this.idBOS = idBOS;
	}

	public String getNameBOS() {
		return nameBOS;
	}

	public void setNameBOS(String nameBOS) {
		this.nameBOS = nameBOS;
	}

	public List<Direction> getListDirectionsChildBOS() {
		return listDirectionsChildBOS;
	}

	public void setListDirectionsChildBOS(List<Direction> listDirectionsChildBOS) {
		this.listDirectionsChildBOS = listDirectionsChildBOS;
	}

	public List<Person> getMembersBOS() {
		return membersBOS;
	}

	public void setMembersBOS(List<Person> membersBOS) {
		this.membersBOS = membersBOS;
	}

	public BOC getAssociatedBOC() {
		return associatedBOC;
	}

	public void setAssociatedBOC(BOC associatedBOC) {
		this.associatedBOC = associatedBOC;
	}

	public String getDescriptionBOS() {
		return descriptionBOS;
	}

	public void setDescriptionBOS(String descriptionBOS) {
		this.descriptionBOS = descriptionBOS;
	}

	public void setNiveauBOS(int niveauBOS) {
		this.niveauBOS = niveauBOS;
	}

	public int getNiveauBOS() {
		return niveauBOS;
	}

	public void setRowKeyBOS(String rowKeyBOS) {
		this.rowKeyBOS = rowKeyBOS;
	}

	public String getRowKeyBOS() {
		return rowKeyBOS;
	}
}
