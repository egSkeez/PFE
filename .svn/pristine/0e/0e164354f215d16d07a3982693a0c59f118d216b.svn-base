				if (typeUserDes.equals("Interne-Person")) {

					do {

						idDest = vb.getCopyLdapListUser().get(k).getId();

						if (idDest == idUserDes) {
							findPersonDest = true;
							personneRechercheDes = vb.getCopyLdapListUser()
									.get(k);
