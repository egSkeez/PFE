											oncomplete="Richfaces.hideModalPanel('msgPanalCAff');"
											type="submit" ajaxSingle="true" styleClass="submitButton"
											action="#{utilisateurGestionBean.initializeaffect}" />
									</div>
								</h:form>
							</rich:modalPanel>
				</rich:panel>
				</div>
			</div>
			<div id="footer">
				<ui:include src="../framGraphique/footer.xhtml"></ui:include>
			</div>
		</div>
	</f:view>
</body>
