<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-
transitional.dtd">

<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich">
	<html>
<head>
<title>Transmission Gestion</title>

<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<script src="../framGraphique/#{languageManager.calendar}"
	type="text/javascript"></script>
</head>
<body>
	<!-- ********************************* -->
	<f:view locale="#{languageManager.locale}">
		<link rel="STYLESHEET" type="text/css"
			href="../framGraphique/#{languageManager.css}" title="css"></link>
		<div id="page">
			<div id="header">
				<ui:include src="../framGraphique/header.xhtml"></ui:include>
			</div>
			<div id="menuPrincipale">
				<ui:include src="../framGraphique/toolbar.xhtml"></ui:include>
			</div>
			<div id="contenu">
				<div id="corpsGauche">
					<rich:panel styleClass="rich-panel">
						<h:form>
							<div id="barreIcone">
								
								<div id="barreIconeDroite">
									<a4j:commandLink id="helpButton"
										action="#{aideEnLigneBean.resultat}"
										oncomplete="#{rich:component('aidePanal')}.show();"
										reRender="aidePanal">
										<h:graphicImage id="j"
											value="../framGraphique/images/help.png" />
									</a4j:commandLink>
									<rich:toolTip for="helpButton" value="#{msg.help}" />
								</div>
							</div>
							<a4j:keepAlive beanName="administrationNatureBean" />
						</h:form>
						<rich:modalPanel id="aidePanal" autosized="true" height="500"
							width="800">
							<f:facet name="header">
								<h:panelGrid columns="3" cellpadding="0" cellspacing="0">

									<h:graphicImage value="../framGraphique/images/help_16.png"
										id="help" />
									<h:outputText
										value="#{msg.natureGestionAideInterface} #{aideEnLigneBean.titreInterface}"
										styleClass="xtePopupInformationTextHeader" />
								</h:panelGrid>
							</f:facet>
							<f:facet name="controls">
								<h:panelGroup>
									<h:graphicImage value="../framGraphique/images/close.png"
										width="15" height="15" id="aidehidelink" />
									<rich:componentControl for="aidePanal" attachTo="aidehidelink"
										operation="hide" event="onclick" />
								</h:panelGroup>
							</f:facet>
							<div class="scroll_div">
								<h:form>
									<a4j:keepAlive beanName="aideEnLigneBean" />
									<h:graphicImage rendered="#{variableGlobale.locale eq 'fr_FR'}"
										value="../framGraphique/images/AideEnLigne/GBO-Gestion-Nature.jpg"
										width="750" height="490" id="aidehidelink" />
									<h:graphicImage rendered="#{variableGlobale.locale eq 'ar'}"
										value="../framGraphique/images/AideEnLigne/GBO-Gestion-Nature-AR.png"
										width="750" height="490" id="aidehidelinkAR" />
									<rich:spacer height="10" />
									<table align="center">
										<tr>
											<td align="center"><a4j:commandButton
													value="#{msg.bouttonOk}"
													oncomplete="Richfaces.hideModalPanel('aidePanal');"
													type="submit" ajaxSingle="true" styleClass="submitButton" />
											</td>
										</tr>
									</table>

								</h:form>
							</div>
						</rich:modalPanel>
					</rich:panel>
					

					<rich:panel styleClass="rich-panel">
						<f:facet name="header">
							<h:panelGrid columns="3">
								<h:outputText value="#{msg.listtransmissionfacet}"
									styleClass="xteEnregistrementLabelOutput" />
							</h:panelGrid>
						</f:facet>
						<a4j:keepAlive beanName="administrationTransmissionBean" />
						<h:form>
							<rich:dataTable value="#{administrationTransmissionBean.listTransmissionDM}"
								var="transmission" id="data" rows="10" styleClass="rich-panel">
								<rich:column width="12%" sortBy="#{transmission.transmissionId}"
									filterBy="#{transmission.transmissionId}" filterEvent="onkeyup" label="ref">
									<f:facet name="header">
										<h:outputText value="#{msg.Reference}" />
									</f:facet>
									<div align="center">
										<h:outputText value="#{transmission.transmissionId}" />
									</div>
								</rich:column>
								<rich:column width="20%" sortBy="#{transmission.transmissionLibelle}"
									filterBy="#{transmission.transmissionLibelle}" filterEvent="onkeyup"
									label="label">
									<f:facet name="header">
										<h:outputText value="#{msg.libelle1}" />
									</f:facet>
									<h:outputText value="#{transmission.transmissionLibelle}"
										rendered="#{administrationTransmissionBean.transmissionLibelleShowFr}" />
									<h:outputText value="#{transmission.transmissionLibelleAr}"
										rendered="#{administrationTransmissionBean.transmissionLibelleShowAr}" />
								</rich:column>
								<rich:column sortBy="#{transmission.transmissionDescription}"
									filterBy="#{transmission.transmissionDescription}" filterEvent="onkeyup"
									label="label">
									<f:facet name="header">
										<h:outputText value="#{msg.comment}" />
									</f:facet>
									<h:outputText value="#{transmission.transmissionDescription}"
										rendered="#{administrationTransmissionBean.transmissionDescriptionShowFr}" />
									<h:outputText value="#{transmission.natureDescriptionAr}"
										rendered="#{administrationTransmissionBean.transmissionDescriptionShowAr}" />
								</rich:column>
								<rich:column width="10%">
									<f:facet name="header">
										<h:outputText value="#{msg.operation}" />
									</f:facet>
									<a4j:keepAlive beanName="administrationTransmissionBean" />
									<center>
										<h:panelGrid columns="6">
											<a4j:commandLink id="consult"
												action="#{administrationTransmissionBean.getSelectionRow2}"
												oncomplete="document.location.href='administrationTransmissionGestion.jsf'">
												<h:graphicImage
													value="../framGraphique/images/icone-detail.png"
													alt="Consulter" />
											</a4j:commandLink>
											<rich:toolTip for="consult" value="#{msg.Consult}" />
										</h:panelGrid>

									</center>

								</rich:column>
							</rich:dataTable>
							<div align="center">
								<rich:datascroller for="data" id="scroller" />
								<h:outputText value="#{msg.nbEnregistrement}"
									styleClass="xteNombreEnregistrementLabelOutput" />
								<h:outputText id="record" value="#{administrationTransmissionBean.recordsTransmission}"
									styleClass="xteNombreEnregistrementLabelOutput" />
							</div>
						</h:form>			
					</rich:panel>
				</div>
				<div id="inter" />
				<div id="corpsDroite">

					<div class="monthenc">
						<script type="text/javascript">
//<![CDATA[
            document.write(monthi8[w4s.getMonth()]); //]]>
            </script>
					</div>
					<div class="datebx">
						<script type="text/javascript">
//<![CDATA[
            document.write(s6ix[s3x]); //]]>
            </script>
						<div class="dateenc">
							<a href="http://www.websiteadministrator.com.au/"><script
									type="text/javascript">
//<![CDATA[
            document.write(w4s.getDate()); //]]>
            </script> </a>
						</div>
					</div>
					<div class="dayenc">
						<a href="http://www.websiteadministrator.com.au/"><script
								type="text/javascript">
//<![CDATA[
            document.write(s0x[w4s.getDay()]); //]]>
            </script> </a>
					</div>
					<div class="yearenc">
						<script type="text/javascript">
//<![CDATA[
            document.write(w4s.getFullYear()); //]]>
            </script>
					</div>
				</div>
			</div>



			<div id="footer">
				<ui:include src="../framGraphique/footer.xhtml"></ui:include>
			</div>

		</div>
	</f:view>
</body>
	</html>
</ui:composition>